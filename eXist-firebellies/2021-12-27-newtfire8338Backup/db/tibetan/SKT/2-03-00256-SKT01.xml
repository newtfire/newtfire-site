			<phrase phr_id="2-03-00256-SKT01-000238">kāyavākcittabhedena trividhadvāranirgame</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000121" type="Verse" s_name="54">
			<phrase phr_id="2-03-00256-SKT01-000239">gatyāgati karoty eṣa sarvadehavyavasthitaḥ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000240">nābhāv akārarūpeṇa hrasvas tu prakīrtitaḥ</phrase>
