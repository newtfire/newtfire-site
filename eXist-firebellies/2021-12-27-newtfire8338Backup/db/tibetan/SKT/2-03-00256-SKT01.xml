		<sentence s_id="2-03-00256-SKT01-SID000129" type="Verse" s_name="62">
			<phrase phr_id="2-03-00256-SKT01-000255">aparāḥ kāyavākcittabhedena caturviṃśatiḥ samudāhṛtāḥ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000256">pīṭhādibhedam āśritya sthāne sthāne samāśritāḥ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000257">kākāsyādi yathā pūrvāḥ pātanyādi viśuddhikāḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000130" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000258">ity āha bhagavān vajrī mudraṇahetum</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000131" type="Verse" s_name="63">
