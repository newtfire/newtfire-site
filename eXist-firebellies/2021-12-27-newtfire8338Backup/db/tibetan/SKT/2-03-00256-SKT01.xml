			<phrase phr_id="2-03-00256-SKT01-000221">[samyaksamādhir bhagavān śrīherukaḥ]</phrase>
			<phrase phr_id="2-03-00256-SKT01-000222">anutpannānāṃ kuśalānāṃ dharmāṇām [utpādanaṃ yamadāḍhī]</phrase>
			<phrase phr_id="2-03-00256-SKT01-000223">[utpannānāṃ kuśalānāṃ dharmāṇāṃ saṃrakṣaṇaṃ yamadūtī]</phrase>
			<phrase phr_id="2-03-00256-SKT01-000224">[utpannānām akuśalānāṃ dharmāṇāṃ] prahāṇaṃ yamadaṃṣṭriṇī</phrase>
			<phrase phr_id="2-03-00256-SKT01-000225">anutpannānām akuśalānāṃ dharmānām anutpādanaṃ yamamathanī ceti</phrase>
			<phrase phr_id="2-03-00256-SKT01-000226">ity āha bhagavān vajrī saptatriṃśadbodhipākṣikadharmāṇāṃ devatāviśuddhim</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000115" type="Verse" s_name="48">
			<phrase phr_id="2-03-00256-SKT01-000227">svahṛnmadhyagataṃ padmam aṣṭapatraṃ sakarṇikam</phrase>
			<phrase phr_id="2-03-00256-SKT01-000228">tasya madhye sthitā nāḍī tailavahnisvarūpikā</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000116" type="Verse" s_name="49">
			<phrase phr_id="2-03-00256-SKT01-000229">kadalīpuṣpasaṃkāśaṃ lambamānā tv adhomukhī</phrase>
			<phrase phr_id="2-03-00256-SKT01-000230">tasya madhye sthito vīraḥ sarṣapasthūlamātrakaḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000117" type="Verse" s_name="50">
			<phrase phr_id="2-03-00256-SKT01-000231">hūṃkāro 'nāhato bījaṃ sravantaṃ[= sravat] tuṣārasannibham</phrase>
			<phrase phr_id="2-03-00256-SKT01-000232">vasanta iti vikhyāto dehināṃ hṛdi nandanaḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000118" type="Verse" s_name="51">
			<phrase phr_id="2-03-00256-SKT01-000233">vaḍavānalarūpā tu vārāhī tilakā matā</phrase>
			<phrase phr_id="2-03-00256-SKT01-000234">karmamārutanirdhūtā jvalantī nābhimaṇḍale</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000119" type="Verse" s_name="52">
