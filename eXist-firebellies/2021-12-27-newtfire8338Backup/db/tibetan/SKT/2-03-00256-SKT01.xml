	<text>
		<text_id t_id="2-03-00256-SKT01"/>
		<sentence s_id="2-03-00256-SKT01-SID000001" type="Text Title" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000001">śrīcakrasaṃvarasādhana</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000002" type="Homage" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000002">oṃ namo buddhāya</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000003" type="Verse" s_name="1">
			<phrase phr_id="2-03-00256-SKT01-000003">natvā pretāsanasthaṃ trinayanam abhitovaktram āraktagātrīṃ vārāhīṃ mūladorbhyāṃ sapadi vidalitaṃ nāganātḥaṃ parābhyām</phrase>
			<phrase phr_id="2-03-00256-SKT01-000004">śeṣaiḥ khaṭvāṅgapāśau narakam ajaśiro vāmadorbhiś caturbhiḥ savyaiḥ karttṛtriśūlaṃ paraśuḍamarukaṃ bibhrataṃ herukākhyam</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000004" type="Verse" s_name="2">
			<phrase phr_id="2-03-00256-SKT01-000005">tatsādhanam ahaṃ vaksye gopitaṃ mlecchabhāṣayā</phrase>
			<phrase phr_id="2-03-00256-SKT01-000006">cakrasaṃvaraguhyārthaṃ siddhimantrasamuccayam</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000005" type="Verse" s_name="3">
			<phrase phr_id="2-03-00256-SKT01-000007">hṛdīndubījabhāvyūhair āhṛtān sugatān puraḥ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000008">sampūjya sarvapūjābhiḥ paṭhed ratnatrayādikām</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000006" type="Verse" s_name="4">
			<phrase phr_id="2-03-00256-SKT01-000009">vibhāvya sakalaṃ śūnyaṃ viśvābjasūryavajrahūṃ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000010">nirgatya[= nirgamayya] ḍākinīvṛndaiḥ kuryāt kīlādikaṃ sudhīḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000007" type="Verse" s_name="5">
			<phrase phr_id="2-03-00256-SKT01-000011">khadhātau cintayed dhīmāṃś caturbhūtavibhāvanām</phrase>
			<phrase phr_id="2-03-00256-SKT01-000012">sarvabhūtasamājena yato viśvāśrayo bhavet</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000008" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000013">tasyāyaṃ kramaḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000009" type="Verse" s_name="6">
			<phrase phr_id="2-03-00256-SKT01-000014">goṣṭhotpalikā caiva śeṣāśuṣkaśirāstrataḥ[= -śiro'strataḥ]</phrase>
			<phrase phr_id="2-03-00256-SKT01-000015">mahāśaṅkhamayaṃ kuryād abhedyaṃ kacchapasya tu</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000010" type="Verse" s_name="7">
			<phrase phr_id="2-03-00256-SKT01-000016">caturbhujaṃ caturvaktraṃ yāval lakṣabhujaṃ tathā</phrase>
			<phrase phr_id="2-03-00256-SKT01-000017">sitadehamahākrodhaṃ vajraghaṇṭāṅgaliṅgitam</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000011" type="Verse" s_name="8">
			<phrase phr_id="2-03-00256-SKT01-000018">kapālamālinaṃ vīraṃ bhasmagātrānulepanam</phrase>
			<phrase phr_id="2-03-00256-SKT01-000019">īṣaddaṃṣṭrākarālāsyaṃ mahāpretakṛtāsanam</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000012" type="Verse" s_name="9">
			<phrase phr_id="2-03-00256-SKT01-000020">jinaraśmimahāmeghaṃ visphurantaṃ samantataḥ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000021">ṣaṇmudrālaṃkṛtaṃ dehaṃ jaṭāmakuṭamaulinam</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000013" type="Verse" s_name="10">
			<phrase phr_id="2-03-00256-SKT01-000022">asitaṃ dakṣiṇaṃ vaktraṃ vāmaṃ raktaṃ bhayānakam</phrase>
			<phrase phr_id="2-03-00256-SKT01-000023">mūlamukhaṃ mahāśvetaṃ paścimaṃ kanakojjvalam</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000014" type="Verse" s_name="11">
			<phrase phr_id="2-03-00256-SKT01-000024">kālamṛtyujayaṃ caiva aṅge khaṭvāṅgayojitam</phrase>
			<phrase phr_id="2-03-00256-SKT01-000025">kapālaṃ cārghapātraṃ ca vaktre astraniyojitam</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000015" type="Verse" s_name="12">
			<phrase phr_id="2-03-00256-SKT01-000026">tasya prajñā mahāghorā khaṇḍamaṇḍitamekhalā</phrase>
			<phrase phr_id="2-03-00256-SKT01-000027">kapālaṃ tv antrasaṃpūrṇaṃ tadvarṇāyudhadhāriṇī</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000016" type="Verse" s_name="13">
			<phrase phr_id="2-03-00256-SKT01-000028">girigahvarakuñjeṣu mahodadhitaṭeṣu vā</phrase>
			<phrase phr_id="2-03-00256-SKT01-000029">ādisiddhiśmaśāne ca tatra maṇḍalam ālikhet</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000017" type="Verse" s_name="14">
			<phrase phr_id="2-03-00256-SKT01-000030">saṃpuṭaṃ saṃpuṭībhāvaṃ vajraśṛṅkhalam ucyate</phrase>
			<phrase phr_id="2-03-00256-SKT01-000031">vajreṇa bhedayet padmaṃ picchakavad bhrāmayet tataḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000018" type="Verse" s_name="15">
			<phrase phr_id="2-03-00256-SKT01-000032">ākarṣayed aṅkuśenaiva kṣobhaṇe muṣalena tu</phrase>
			<phrase phr_id="2-03-00256-SKT01-000033">grasane vajram evākhyaṃ vidhinā sphārayet tataḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000019" type="Verse" s_name="16">
			<phrase phr_id="2-03-00256-SKT01-000034">āvedhaśara[= -śareṇa] saṃyojya dravībhūtaṃ savidyayā[= saha vidyayā]</phrase>
			<phrase phr_id="2-03-00256-SKT01-000035">manthyamanthānasaṃyogād dharmakāyaṃ yathā tathā</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000020" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000036">sarvabuddhāḥ samājena codayāmāsu[= codayāmāsur] māṃ laghu</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000021" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000037">oṃ mahāsukhavajrasattva jaḥ hūṃ vaṃ hoḥ suratas tvaṃ dṛśya ho</phrase>
			<phrase phr_id="2-03-00256-SKT01-000038">iti</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000022" type="Verse" s_name="17">
			<phrase phr_id="2-03-00256-SKT01-000039">saṃcoditamātre tu-m api[=&gt; tu mayi] lokadhātusukhaprade</phrase>
			<phrase phr_id="2-03-00256-SKT01-000040">nijamantrāt svayaṃ bhūyād yathodayavidhikramaiḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000023" type="Verse" s_name="18">
			<phrase phr_id="2-03-00256-SKT01-000041">aṃhūṃbhyāṃ tato vajrī utthito dravamūrtitaḥ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000042">nīlavarṇo mahāghoro raktajvālābhamaṇḍalaḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000024" type="Verse" s_name="19">
			<phrase phr_id="2-03-00256-SKT01-000043">caturvaktro mahābhīmo dviṣaḍbhujavirājitaḥ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000044">umāpatiṃ samākramya divaseśvaraviṣṭaraḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000025" type="Verse" s_name="20">
			<phrase phr_id="2-03-00256-SKT01-000045">mūlamukhaṃ mahākṛṣṇaṃ dakṣiṇaṃ kundasannibham</phrase>
			<phrase phr_id="2-03-00256-SKT01-000046">vāmaṃ raktaṃ mahābhīmaṃ paścimaṃ kanakojjvalam</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000026" type="Verse" s_name="21">
			<phrase phr_id="2-03-00256-SKT01-000047">pūrvāyudhasamāyuktas trinetrais tu virājitaḥ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000048">pratyālīdhapadasthas tu saroṣahasitānanaḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000027" type="Verse" s_name="22">
			<phrase phr_id="2-03-00256-SKT01-000049">tasya prajñā vārāhī raktavarṇā mahojjvalā</phrase>
			<phrase phr_id="2-03-00256-SKT01-000050">dvibhujaikānanā divyā vajranārakapālinī</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000028" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000051">bhagavataś caturmukhād adhiṣṭhānam</phrase>
			<phrase phr_id="2-03-00256-SKT01-000052">oṃ sumbha nisumbha huṃ hūṃ phaṭ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000053">oṃ gṛhna 2 huṃ hūṃ phaṭ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000054">oṃ gṛhnāpaya 2 huṃ hūṃ phaṭ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000055">oṃ ānaya ho bhagavan[=&gt; bhagabāṃ] vidyārāja huṃ hūṃ phaṭ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000056">vilomataḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000029" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000057">oṃ dharmadhātusvabhāvātmako 'ham</phrase>
			<phrase phr_id="2-03-00256-SKT01-000058">iti sevā</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000030" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000059">oṃ ha</phrase>
			<phrase phr_id="2-03-00256-SKT01-000060">vajrasattvo hṛdaye</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000031" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000061">oṃ nama[=&gt; namaḥ] hi</phrase>
			<phrase phr_id="2-03-00256-SKT01-000062">vairocanaḥ śirasi</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000032" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000063">oṃ svāhā hu</phrase>
			<phrase phr_id="2-03-00256-SKT01-000064">padmanarteśvaraḥ śikhāyām</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000033" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000065">oṃ vauṣaṭ he</phrase>
			<phrase phr_id="2-03-00256-SKT01-000066">śrīherukaḥ kakṣadvaye</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000034" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000067">oṃ hūṃ hūṃ ho</phrase>
			<phrase phr_id="2-03-00256-SKT01-000068">vajrasūryaś cakṣurdvaye</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000035" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000069">oṃ phaṭ haṃ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000070">hayagrīvaḥ sarvāṅgeṣu</phrase>
			<phrase phr_id="2-03-00256-SKT01-000071">astram</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000036" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000072">oṃ vaṃ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000073">vajravārāhī nābhau</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000037" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000074">oṃ hāṃ yoṃ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000075">yāminī hṛdaye</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000038" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000076">oṃ hrīṃ moṃ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000077">mohinī lalāṭe</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000039" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000078">oṃ hreṃ hrīṃ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000079">saṃcāriṇī śirasi</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000040" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000080">oṃ hūṃ hūṃ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000081">saṃtrāsinī śikhāyām</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000041" type="Sentence" s_name="24">
			<phrase phr_id="2-03-00256-SKT01-000082">oṃ phaṭ phaṭ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000083">devyāḥ sarvāṅgeṣu</phrase>
			<phrase phr_id="2-03-00256-SKT01-000084">ity upasādhanam</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000042" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000085">hṛdi hūṃkāreṇa candrasthaṃ cittavajram</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000043" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000086">oṃ hrīḥ ha ha hūṃ hūṃ phaṭ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000087">vāgvajraṃ kaṇṭhe</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000044" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000088">oṃ śrīvajra-he-he-ru-ru-kaṃ hūṃ hūṃ phaṭ dākinījālasamvaraṃ svāhā</phrase>
			<phrase phr_id="2-03-00256-SKT01-000089">lalāṭe kāyavajram</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000045" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000090">oṃ sarvabuddhaḍākinīye vajravarṇanīye hūṃ hūṃ phaṭ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000091">devyā hṛdaye</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000046" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000092">oṃ caurī hūṃ hūṃ phaṭ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000093">kaṇṭhe</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000047" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000094">oṃ vajravairocanīye svāhā</phrase>
			<phrase phr_id="2-03-00256-SKT01-000095">lalāṭe</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000048" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000096">śirasi oṃkāraṃ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000049" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000097">hṛdaye hūṃkāraṃ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000050" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000098">kiñjalke āḥkāraṃ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000051" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000099">kaṭibandhe svākāram</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000052" type="Sentence" s_name="25">
			<phrase phr_id="2-03-00256-SKT01-000100">ūruyugme hākāram</phrase>
			<phrase phr_id="2-03-00256-SKT01-000101">iti sādhanam</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000053" type="Verse" s_name="26">
			<phrase phr_id="2-03-00256-SKT01-000102">jñānasamayasambhūtas tato vajrī ihocyate</phrase>
			<phrase phr_id="2-03-00256-SKT01-000103">abhiṣekapadaṃ dhyātvā paścāt pūjāṃ samārabhet</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000054" type="Verse" s_name="27">
			<phrase phr_id="2-03-00256-SKT01-000104">mantraṃ saṃyojya dvau liṅge[= dvayor liṅgayoḥ]</phrase>
			<phrase phr_id="2-03-00256-SKT01-000105">akāram nyasya naranāsikāyāṃ vaṃkāraṃ saṃyojya tathā liṅgavaraṭake</phrase>
			<phrase phr_id="2-03-00256-SKT01-000106">yoginīṃ manthayed dhīmān śuddhakāyo nirāmayaḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000055" type="Sentence" s_name="28">
			<phrase phr_id="2-03-00256-SKT01-000107">ity ādiyogo nāma samādḥiḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000056" type="Sentence" s_name="">
			<phrase phr_id="2-03-00256-SKT01-000108">oṃ sarvatathāgatānurāgavajrasvabhāvātmako 'ham</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000057" type="Verse" s_name="29">
			<phrase phr_id="2-03-00256-SKT01-000109">hṛdbījabhābhir ākṛṣya sugatān</phrase>
