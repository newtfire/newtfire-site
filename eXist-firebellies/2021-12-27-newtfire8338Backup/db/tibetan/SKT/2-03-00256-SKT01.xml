		<sentence s_id="2-03-00256-SKT01-SID000126" type="Verse" s_name="59">
			<phrase phr_id="2-03-00256-SKT01-000249">dalānāṃ catuṣkeṣu caturdikṣu vyavasthitāh</phrase>
			<phrase phr_id="2-03-00256-SKT01-000250">catasro bhūtanāḍyas tu tailavahnisvarūpam āśritāḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000127" type="Verse" s_name="60">
			<phrase phr_id="2-03-00256-SKT01-000251">vidikṣu vyavasthitā nāḍyaś catasras tadgatā api</phrase>
			<phrase phr_id="2-03-00256-SKT01-000252">pañcāmṛtavahās tās tu tatpūjārūpatāṃ gatāḥ</phrase>
		</sentence>
		<sentence s_id="2-03-00256-SKT01-SID000128" type="Verse" s_name="61">
			<phrase phr_id="2-03-00256-SKT01-000253">catuḥpūjeti vikhyātās tadrūpā eva bhāvataḥ</phrase>
			<phrase phr_id="2-03-00256-SKT01-000254">iti dehasya hṛnmadhye aṣṭa nāḍyo vyavasthitāḥ</phrase>
